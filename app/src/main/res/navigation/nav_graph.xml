            app:destination="@id/profileFragment" />
    </fragment>
    <fragment
        android:id="@+id/favoritesFragmentId"
        android:name="com.example.fitnessapp.fragments.FavoritesFragment"
        android:label="fragment_favorites"
        tools:layout="@layout/fragment_favorites" />
    <fragment
        android:id="@+id/categoriesFragmentId"
        android:name="com.example.fitnessapp.fragments.CategoriesFragment"
        android:label="fragment_categories"
        tools:layout="@layout/fragment_categories" />
    <fragment
        android:id="@+id/fragment_profile"
        android:name="com.example.fitnessapp.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />

